		"builtin-modules": "3.3.0",
		"esbuild": "0.20.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-unused-imports": "^3.1.0",
		"obsidian": "latest",
